{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "users": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "username":{
            "type": "string"
          },
          "email": {
            "format": "email",
            "type": "string"
          },
          "created_at":{
            "format": "date-time",
            "type": "string"
          }
        },
        "required" :["id", "username", "email", "created_at", "tasks"]
      }
    }
  }
}